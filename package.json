  "description": "The dumb alternative to markdown. The keyword for title is title. Includes Scroll static publishing software.",
  "main": "index.js",
  "scripts": {
    "test": "tap",
    "coverage": "tap --coverage-report=lcov"
  },
  "bin": {
    "scroll": "./scroll/scroll.node.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/treenotation/dumbdown.git"
  },
  "author": "Breck Yunits",
  "bugs": {
