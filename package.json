  },
  "prettier": {
    "useTabs": false,
    "tabWidth": 2,
    "semi": false,
    "printWidth": 240
  },
  "homepage": "https://github.com/treenotation/dumbdown#readme",
  "dependencies": {
    "fs-extra": "^9.1.0",
    "jtree": "^51.3.0",
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "tap": "^14.11.0"
  }
